{application,ex_brotli,
    [{modules,['Elixir.ExBrotli','Elixir.ExBrotli.DigesterCompressor']},
     {compile_env,
         [{rustler_precompiled,[force_build,ex_brotli],error},
          {rustler_precompiled,[force_build_all],error}]},
     {optional_applications,[rustler,phoenix]},
     {applications,
         [kernel,stdlib,elixir,logger,rustler_precompiled,rustler,phoenix]},
     {description,"Elixir wrapper for Rust based brotli compression"},
     {registered,[]},
     {vsn,"0.5.0"}]}.
